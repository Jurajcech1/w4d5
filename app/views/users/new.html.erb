<form action="<%= users_url %>" method="post">

<%= auth_token_input %>

  <label for="user_name">username</label>
  <input
    type="text"
    name="user[user_name]"
    id = "user_name"
    value="<%= @user.user_name %>"
    >
    <br>

    <label for="password">password</label>
    <input
      type="password"
      name="user[password]"
      id = "password"
      >

    <br>
    <input type="submit" value="sign up">
</form>
