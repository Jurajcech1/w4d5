<h1>New Sub</h1>
<form action="<%= subs_url %>" method="post">

<%= auth_token_input %>

  <label for="sub_title"><h4>Sub Title</h4></label>
  <input
    type="text"
    name="sub[title]"
    id = "sub_title"
    value="<%= @sub.title %>"
    >
    <br>

    <label for="description">description</label>
    <textarea id="description" name="sub[description]"> <%= @sub.description %> </textarea>

    <br>
    <input type="submit" value="create sub">
</form>
